<!DOCTYPE html>
<html>
  <% include apiConsole/head %>
  <script type="text/javascript" src='/packageRepository.js'> </script>
  
  <body>
    <% include apiConsole/header %>
    <% include apiConsole/methods %>    

    <script type="text/javascript">

    $(function() {      
        $("#infoEvent").text("Waiting for events ...");

        var ar = new PackageRepository ();
        ar.addApplicationChangedListener (function (aData) {          
          $("#infoEvent").append("<br />").append(aData);
        });
    });
   </script>

  </body>

</html>