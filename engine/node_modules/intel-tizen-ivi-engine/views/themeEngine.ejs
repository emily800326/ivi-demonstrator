<!DOCTYPE html>
<html>
  <% include apiConsole/head %>
  <script type="text/javascript" src='/apiEndpointBase.js'> </script>
  <script type="text/javascript" src='/themeengine.js'> </script>
  
  <body>
    <% include apiConsole/header %>      

    <fieldset>
        <legend>Stylesheet locations:</legend>
        &lt;link rel='stylesheet' href='<a href="/css/car/car.css">/css/car/car.css</a>' /&gt; <br/>
        &lt;link rel='stylesheet' href='<a href="/css/user/user.css">/css/user/user.css</a>' /&gt; <br/>
        &lt;script type="text/javascript" src='<a href="/css/car/car.js">/css/car/car.js</a>' &gt; &lt;/script&gt; <br/>
        &lt;script type="text/javascript" src='<a href="/css/user/user.js">/css/user/user.js</a>' &gt; &lt;/script&gt;
    </fieldset>

    <% include apiConsole/methods %>    

    <script type="text/javascript">

    $(function() {      
        var te = new ThemeEngine ();  
        te.addStatusListener(function (aData) {
          document.getElementById('container_frame').contentWindow.location.reload();
          $("#statusEvent").text(aData);
        });

        $("#statusEvent").text("Waiting for events ...");
    });
   </script>
       
    <h2>Test container</h2>
    <iframe id="container_frame" src="./themeEngineContainer.html">
    </iframe>

  </body>

</html>