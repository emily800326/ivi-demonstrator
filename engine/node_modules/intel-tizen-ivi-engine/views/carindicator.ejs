<!DOCTYPE html>
<html>
  <% include apiConsole/head %>
  <script type="text/javascript" src='/apiEndpointBase.js'> </script>
  <script type="text/javascript" src='/carindicator.js'> </script>
  
  <body>
    <% include apiConsole/header %>      
    <% include apiConsole/methods %>    

    <script type="text/javascript">

    $(function() {      
        var te = new CarIndicator ();
        var updateStatus = function (aData) {          
          $("#statusEvent").text(JSON.stringify(aData));
        };
        te.addStatusListener(updateStatus);
        te.getStatus(updateStatus);
    });
   </script>

  </body>

</html>