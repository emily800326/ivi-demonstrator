<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    Available API services:

    <ul>
    <% services.forEach(function(aService) { %>
      <li>
        <a href="/<%= aService.endpointName %>"><%= aService.description %></a>
      </li>
    <% }); %>
    </ul>

    Available applications:

    <ul>
      <% applications.forEach(function(anApplication) { %>
        <li>
          <a href="/application/<%= anApplication.__applicationDirectory %>/index.html<%= (launchWithPersistentConsole ? "?persistentConsole" : "") %>"> 
            <%= anApplication.name %> </a>          
        </li>
      <%}); %>
    </ul>

  </body>
</html>